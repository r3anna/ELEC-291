
'david
Dim  DVoice
Set DVoice = CreateObject("SAPI.SpVoice")
Set DVoice.Voice = DVoice.GetVoices.Item(0)
DVoice.Rate = 0
DVoice.Volume = 99
DVoice.Speak "Hello, my name is David"

'zira
Dim  ZVoice
Set ZVoice = CreateObject("SAPI.SpVoice")
Set ZVoice.Voice = ZVoice.GetVoices.Item(1)
ZVoice.Rate = 0
ZVoice.Volume = 99
ZVoice.Speak "Hello, my name is Zira"


'sabina
Dim  SVoice
Set SVoice = CreateObject("SAPI.SpVoice")
Set SVoice.Voice = ZVoice.GetVoices.Item(2)
SVoice.Rate = -1
SVoice.Volume = 100
SVoice.Speak "Hola, mi nombre es Sabina"

